create table newExpert  select * from originExpert  group by ZJ_NO;
delete from newExpert where dw_guid="" or ZJ_NO=""  or ZJ_NAME = "";

update newExpert set dw_guid='武汉大学动物实验中心 - ABSL-Ⅲ实验室' where zj_no=250537;
update newExpert set dw_guid='四川大学华西医院 - 临床医学院' where dw_guid like '四川大学华西医院/临床医学院%';
update newExpert set dw_guid='湖北民族学院' where dw_guid like '湖北民族学院***';
update newExpert set dw_guid='杭州电子科技大学机械工程学院' where dw_guid = '杭州电子科技大学机械工程学院*';


